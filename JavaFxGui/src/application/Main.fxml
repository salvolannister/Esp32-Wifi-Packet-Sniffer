            <Button mnemonicParsing="false" prefHeight="156.0" prefWidth="210.0" text="Time diagram" BorderPane.alignment="CENTER" />
         </right>
         <bottom>
            <Button mnemonicParsing="false" prefHeight="156.0" prefWidth="210.0" text="Show Mac's Frequency" BorderPane.alignment="CENTER" />
